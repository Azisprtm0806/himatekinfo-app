<section id="proker" class="faq section-bg">
  <div class="container" data-aos="fade-up">

    <div class="section-title">
      <h2>Program Kerja</h2>
    </div>

    <div class="faq-list">
      <ul>

        <% for( let i = 0; i < proker.length; i++ ) { %>
          <li data-aos="fade-up" data-aos-delay="200">
            <i class="bx bxs-circle icon-help"></i> <a data-bs-toggle="collapse" data-bs-target="#faq-list-<%= i+1 %>" class="collapsed"><%= proker[i].namaProker %><i class="bx bx-chevron-down icon-show"></i><i class="bx bx-chevron-up icon-close"></i></a>
            <div id="faq-list-<%= i+1 %>" class="collapse" data-bs-parent=".faq-list">
              <p>
                <%= proker[i].deskripsi %>
              </p>
            </div>
          </li>
        <% } %>
      </ul>
    </div>

  </div>
</section><!-- End Frequently Asked Questions Section -->